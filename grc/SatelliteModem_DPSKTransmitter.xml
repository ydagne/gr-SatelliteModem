<?xml version="1.0"?>
<block>
  <name>DPSKTransmitter</name>
  <key>SatelliteModem_DPSKTransmitter</key>
  <category>SatelliteModem</category>
  <import>import SatelliteModem</import>
  <make>SatelliteModem.DPSKTransmitter($amplitude, $excessBW, $sps, $arity)</make>

  <param>
    <name>Amplitude</name>
    <key>amplitude</key>
    <type>float</type>
  </param>

  <param>
    <name>Excess BW (alpha)</name>
    <key>excessBW</key>
    <type>float</type>
  </param>

  <param>
    <name>Samples/symbol</name>
    <key>sps</key>
    <type>float</type>
  </param>


  <param>
    <name>Arity</name>
    <key>arity</key>
    <type>int</type> 
  </param>
  

  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>
  
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
