<?xml version="1.0"?>
<block>
  <name>Framer</name>
  <key>SatelliteModem_Framer</key>
  <category>SatelliteModem</category>
  <import>import SatelliteModem</import>
  <make>SatelliteModem.Framer($(id)_msgq_in, $MTU, $nonBlocking)</make>
  
  <param>
    <name>MTU</name>
    <key>MTU</key>
    <type>int</type>
  </param>


  <param>
    <name>Non-Blocking</name>
    <key>nonBlocking</key>
    <type>int</type>
     <option>
       <name>Yes</name>
       <key>1</key>
     </option>
     <option>
       <name>No</name>
       <key>0</key>
     </option>
  </param>

  
  <sink>
    <name>in</name>
    <type>msg</type>
  </sink>

  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
