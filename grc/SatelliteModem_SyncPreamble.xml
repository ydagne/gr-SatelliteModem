<?xml version="1.0"?>
<block>
  <name>SyncPreamble</name>
  <key>SatelliteModem_SyncPreamble</key>
  <category>SatelliteModem</category>
  <import>import SatelliteModem</import>
  <make>SatelliteModem.SyncPreamble($payloadLength, $overheadBytes)</make>

  <param>
    <name>Payload Length</name>
    <key>payloadLength</key>
    <type>int</type>
  </param>


  <param>
    <name>Preamble overhead (bytes)</name>
    <key>overheadBytes</key>
    <type>int</type>
  </param>


  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>


  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
